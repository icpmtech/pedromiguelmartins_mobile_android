        android:label="@string/app_name"
        android:parentActivityName=".MainHomeActivity"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:name=".ProjectsActivity"
            android:label="@string/title_activity_projects"
            android:parentActivityName=".MainHomeActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <category android:name="android.intent.category.DEFAULT" />

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="net.azurewebsites.pedromiguelmartins.pedromiguelmartins.MainHomeActivity" />
